<%= form_with model:[@rooom][@review][@comment], local: true do |f| %>

<div class="field">
<%= f.label :name %><br>
<%= f.text_field :name %>
</div>

<div class="field">
<%= f.label :body %><br>
<%= f.text_area :body %>
</div>

<%= f.hidden_field :review_id %>

<div>
<%= f.submit '登録する', class:"button" %>
</div>

<% end %>
